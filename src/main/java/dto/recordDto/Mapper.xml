   <result property="DunkKick" column="DunkKick" />
   <result property="ConventionalDeadLift" column="ConventionalDeadLift" />
</resultMap>






<resultMap type="dto.recordDto.BackDto" id="BackMap">
   <result property="backId" column="backId" />
   <result property="Pullup" column="Pullup" />
   <result property="LatPullDown" column="LatPullDown" />
   <result property="SittedLow" column="SittedLow" />
   <result property="BabelLow" column="BabelLow" />
   <result property="OneArmLow" column="OneArmLow" />
   <result property="RomanianDeadlift" column="RomanianDeadlift" />
